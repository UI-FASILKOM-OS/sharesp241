#!/bin/bash
# REV01: Mon 19 Feb 2024 15:00
# START: Tue 06 Feb 2024 15:00

WEEK="W01"

for II in * ; do
  [ -d $II/ATTENDANCE/ ] && {
    [ -d $II/$WEEK/ ] && {
      echo "==== ==== ==== $II/$WEEK/"
      git log --pretty=format:'%ad' --date=format:'%Y%m%d %H%M' $II/$WEEK/ 
      echo ""
    }
  }
done
